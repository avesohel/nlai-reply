{
  "name": "youtube-reply-service",
  "version": "1.0.0",
  "description": "MVP subscription-based YouTube comments reply service",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "test": "jest",
    "test:setup": "node scripts/test-setup.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "seed": "node scripts/seed.js",
    "setup": "./setup.sh"
  },
  "keywords": ["youtube", "comments", "automation", "saas", "subscription"],
  "author": "Ali Sohel <avesohel@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "googleapis": "^126.0.1",
    "stripe": "^13.3.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^6.8.1",
    "morgan": "^1.10.0",
    "joi": "^17.9.2",
    "nodemailer": "^6.9.4",
    "cron": "^2.4.3",
    "compression": "^1.7.4",
    "openai": "^4.20.1",
    "pinecone-client": "^1.1.0",
    "@pinecone-database/pinecone": "^1.1.2",
    "youtube-transcript": "^1.0.6",
    "node-html-parser": "^6.1.10",
    "sentiment": "^5.0.2",
    "natural": "^6.12.0",
    "tiktoken": "^1.0.10",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^8.2.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "eslint": "^8.46.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}